<div data-id="{{_id}}" data-parentId="{{_parentId}}" class="editor-menu-item-inner clearfix no-select">
  <div class="editor-menu-item-settings handle">
    <div class="editor-menu-item-settings-inner">
      <button class="open-context-contentObject"><i class="fa fa-cog fa-fw"></i></button>
      <button class="contentObject-delete editor-delete-contentobject-element" title="{{ t 'app.clicktodelete'}}"><i class="fa fa-trash-o"></i></button>
    </div>
  </div>
  <div class="editor-menu-item-content">
    <div class="editor-menu-item-icon {{_type}}">
      {{#ifValueEquals _type "menu"}}
        <i class="fa fa-folder-o"></i>
      {{/ifValueEquals}}
      {{#ifValueEquals _type "page"}}
        <i class="fa fa-file-o"></i>
      {{/ifValueEquals}}
    </div>
    <div class="editor-menu-item-title">
      <h3 class="editor-menu-item-title-text">{{{title}}}</h3>
      {{#if menuSettings}}
        {{#if menuSettings._listMenu}}
          {{#if menuSettings._listMenu._pageNumber}}
            <span class="editor-menu-item-title-pn">Page: {{{menuSettings._listMenu._pageNumber}}}</span>
          {{/if}}
        {{/if}}
      {{/if}}

    </div>
  </div>
</div>

<div class="editor-menu-item-pin"></div>
